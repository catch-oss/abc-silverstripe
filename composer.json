{
  "name": "azt3k/abc-silverstripe",
  "description": "Library that adds some useful functionality to Silverstripe",
  "license": "BSD-3-Clause-Clear",
  "type": "silverstripe-vendormodule",
  "authors": [
    {
      "name": "Aaron Latham-Ilari",
      "email": "abcdigital@abcdigital.co.nz"
    }
  ],
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require": {
    "php": "^8.1",
    "silverstripe/framework": "^5",
    "silverstripe/cms": "^5",
    "silverstripe/crontask": "^3.0",
    "psr/simple-cache": "^3.0",
    "composer/installers": "^2.2"
  },
  "autoload": {
    "psr-4": {
      "Azt3k\\SS\\": "src/abc/code/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "silverstripe/vendor-plugin": true
    }
  }
}
